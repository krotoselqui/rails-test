<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<%= link_to "New Product", new_product_path %>

<div id="products">
    <% @products.each do |product| %>
        <div>
            <a href="/products/<%= product.id %>">
                <%= product.name %>
            </a>
            <%= link_to product.name, product %>
        </div>
    <% end %>
<%= debug @products %>
